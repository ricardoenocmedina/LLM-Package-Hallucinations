{
  "rest": {
    "RestGenerator": {
      "name": "tinyllama-wrapper",
      "uri": "http://127.0.0.1:8000/generate",
      "method": "post",
      "headers": {
        "Content-Type": "application/json"
      },
      "req_template_json_object": {
        "text": "$INPUT"
      },
      "response_json": true,
      "response_json_field": "text",
      "request_timeout": 300
    }
  }
}
